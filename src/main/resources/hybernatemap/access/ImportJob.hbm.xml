<?xml version="1.0"?><!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping >  
<class name="it.unitn.disi.diversicon.ImportJob" 
	   table="ImportJob" 
	   node="ImportJob" 
	   dynamic-insert="true">
   
    <id column="id" type="long">
           <generator class="sequence"/>             
    </id>
    
        
   <property name="author" column="author" node="@author" type="string"/>
   <property name="description" column="description" node="@description" type="string"/>   
   <property name="fileUrl" column="fileUrl" node="@fileUrl" type="string"/>
   <property name="lexicalResourceName" column="lexicalResourceName" node="@lexicalResourceName" type="string"/>
   <property name="startDate" column="startDate" node="@startDate" type="date"/>
   <property name="endDate" column="endDate" node="@endDate" type="date"/>
   
   
   
   <list name="logMessages" node=".">
            <key column="importJobId" />
            <index column="idx"/>
            <one-to-many class="it.unitn.disi.diversicon.ImportJob" node="ImportJob"/>
    </list> 
      
  			
</class>

</hibernate-mapping>